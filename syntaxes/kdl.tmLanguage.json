{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "KDL",
	"patterns": [
		{
			"include": "#strings"
		}
	],
	"repository": {
		"strings": {
			"name": "string.quoted.double.kdl",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.kdl",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.kdl"
}
