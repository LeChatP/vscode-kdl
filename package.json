{
    "name": "kdl",
    "displayName": "KDL",
    "description": "Language support for the KDL Document Language",
    "version": "1.2.0",
    "license": "Apache-2.0",
    "publisher": "kdl-org",
    "repository": "https://github.com/kdl-org/vscode-kdl",
    "scripts": {
        "pack": "vsce package",
        "publish": "vsce publish"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "kdl",
                "aliases": [
                    "KDL",
                    "kdl"
                ],
                "extensions": [
                    ".kdl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kdl",
                "scopeName": "source.kdl",
                "path": "./syntaxes/kdl.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.99.0"
    }
}
